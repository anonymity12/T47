<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.thinkpad.t47checklisthead.MainActivity">

<Button
    android:id="@+id/two_btn_style_id"
    android:layout_width="200dp"
    android:layout_height="200dp"
    android:background="@drawable/bg_vectorbutton"
    android:text="Two Caps?"
    android:padding="2dp"
    android:textColor="@color/colorPrimary"
    android:textAllCaps="false"
    android:layout_centerHorizontal="true"

    />
    <LinearLayout
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/ic_x"
        android:layout_below="@+id/two_btn_style_id"
        android:layout_centerHorizontal="true"
        android:layout_margin="30dp"
        ></LinearLayout>


</RelativeLayout>
